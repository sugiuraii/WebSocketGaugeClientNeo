{
    "name": "websocketgaugeclientneo",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/sugiuraii/WebSocketGaugeClientNeo.git"
    },
    "version": "1.0.0",
    "keywords": [
        "util",
        "functional",
        "server",
        "client",
        "browser"
    ],
    "author": "K. Sugiura",
    "contributors": [],
    "dependencies": {
        "@types/jquery": "^3.5.29",
        "@types/react-bootstrap": "^0.32.36",
        "autoprefixer": "^10.4.19",
        "bootstrap": "^5.3.3",
        "bootswatch": "^5.3.3",
        "css-loader": "^6.10.0",
        "file-loader": "^6.2.0",
        "jquery": "^3.7.1",
        "jsonc-parser": "^3.2.1",
        "pixi-gauge": "./packages/pixi-gauge",
        "placement-coordinate-calc": "./packages/placement-coordinate-calc",
        "pixi.js": "^8.0.4",
        "popper.js": "^1.16.1",
        "postcss-loader": "^8.1.1",
        "react": "^18.2.0",
        "react-bootstrap": "^2.10.2",
        "react-dom": "^18.2.0",
        "sass": "^1.72.0",
        "sass-loader": "^14.1.1",
        "style-loader": "^3.3.4",
        "ts-loader": "^9.5.1",
        "typescript": "^5.4.3",
        "url-loader": "^4.1.1",
        "webpack": "^5.91.0",
        "websocket-gauge-client-communication": "./packages/websocket-gauge-client-communication",
        "websocket-gauge-client-communication-service": "./packages/websocket-gauge-client-communication-service",
        "pixi-traillayer": "./packages/pixi-traillayer",
        "basic-logger": "./packages/basic-logger",
        "meter-application-common": "./packages/meter-application-common",
        "interpolation": "./packages/interpolation",
        "@websocketgaugeclientneo/meterparts-analogsinglemeter": "./packages/parts-lib/meterparts-analogsinglemeter",
        "@websocketgaugeclientneo/meterparts-mfdpanel": "./packages/parts-lib/meterparts-mfdpanel",
        "@websocketgaugeclientneo/meterparts-analogmetercluster": "./packages/parts-lib/meterparts-analogmetercluster",
        "@websocketgaugeclientneo/meterparts-ledmeter": "./packages/parts-lib/meterparts-ledmeter"
    },
    "devDependencies": {
        "@types/react": "^18.2.69",
        "@types/react-dom": "^18.2.22",
        "@typescript-eslint/eslint-plugin": "^7.3.1",
        "@typescript-eslint/parser": "^7.3.1",
        "depcheck": "^1.4.7",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-react": "^7.34.1",
        "eslint-plugin-react-hooks": "^4.6.0",
        "npm-run-all": "^4.1.5",
        "prettier": "^3.2.5",
        "rimraf": "^5.0.5",
        "tsconfig-paths-webpack-plugin": "^4.1.0",
        "webpack-cli": "^5.1.4"
    },
    "scripts": {
        "build-full": "npm run build-depends; npm run build-all",
        "build-all": "npm-run-all --parallel build-WebSocketTester build-benchmark build-application build-partsTest build-index",
        "build-WebSocketTester": "cd src/WebSocketTester && npx webpack --progress=profile",
        "build-benchmark": "cd src/benchmark && npx webpack --progress=profile",
        "build-application": "cd src/application && npx webpack --progress=profile",
        "build-partsTest": "cd src/test/partsTest && npx webpack --progress=profile",
        "build-index": "cd src/index && npx webpack --progress=profile",
        "build-special-assettocorsa": "cd src/special/assettocorsaApp && npx webpack --progress=profile",
        "debug-watch-benchmark": "cd src/benchmark && npx webpack --watch --progress=profile",
        "debug-watch-application": "cd src/application && npx webpack --watch --progress=profile",
        "debug-watch-partsTest": "cd src/test/partsTest && npx webpack --watch --progress=profile",
        "build-depends": "npm-run-all --parallel build-depends:* build-parts",
        "build-parts:analogmetercluster" : "npm run -w packages/parts-lib/meterparts-analogmetercluster build",
        "build-parts:analogsinglemeter" : "npm run -w packages/parts-lib/meterparts-analogsinglemeter build",
        "build-parts:ledmeter" : "npm run -w packages/parts-lib/meterparts-ledmeter build",
        "build-parts:mfdpanel" : "npm run -w packages/parts-lib/meterparts-mfdpanel build",
        "build-parts" : "npm-run-all --parallel build-parts:*",
        "build-depends:placement-coordinate-calc" : "npm run -w packages/placement-coordinate-calc build",
        "build-depends:pixi-gauge" : "npm run -w packages/pixi-gauge build",
        "build-depends:pixi-traillayer" : "npm run -w packages/pixi-traillayer build",
        "build-depends:basic-logger" : "npm run -w packages/basic-logger build",
        "build-depends:interpolation" : "npm run -w packages/interpolation build",
        "build-depends:websocket-gauge-client-communication" : "npm run -w packages/websocket-gauge-client-communication build; npm run -w packages/websocket-gauge-client-communication-service build",
        "build-depends:meter-application-common" : "npm run -w packages/meter-application-common build"
        },
    "workspaces": [
        "packages/*",
        "packages/parts-lib/*"
    ]
}
